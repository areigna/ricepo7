<div class="page-content">
    <div class="list-block">
        <ul>
            {{#each this}}
                <li>
                    <a href="#" class="item-link item-content">
                        <!--
                        <div class="item-media">
                            <i class="icon my-icon"></i>
                        </div>
                        -->
                        <div class="item-inner">
                            <div class="item-title">
                                {{chn_name}}
                            </div>
                            <div class="item-after">
                                {{#if closed}}
                                closed
                                {{else}}
                                open
                                {{/if}}
                            </div>
                        </div>
                    </a>
                </li>
            {{/each}}
        </ul>
    </div>
</div>
