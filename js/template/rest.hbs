{{#each this}}
<li>
    <a href="food.html?rest_id={{rest_id}}" data-rest = "{{this}}" class="item-link item-content">
        <!--
        <div class="item-media">
            <img src="path/to/img.jpg">
        </div>
        -->
        <div class="item-inner">
            <div class="item-title-row">
                <div class="item-title">
                    {{chn_name}}
                </div>
                <div class="item-after">
                    {{#if closed}}
                        closed
                    {{else}}
                        open
                    {{/if}}
                </div>
            </div>
            <div class="item-subtitle">{{min}}</div>
            <div class="item-text">{{delivery_fee}}</div>
        </div>
    </a>
</li>
{{/each}}
